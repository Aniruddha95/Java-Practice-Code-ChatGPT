// fibonacci series

public class Main {
    public static void main(String[] args) {
        String series = fib(10);
        System.out.println(series);

    }
    static String fib(int n) {

        int min = 0;
        int max = 1;
        int num;
        String result = 0 +" "+ 1 +" ";

        while (n - 2 > 0) {
          num = min + max;
          min = max;
          max = num;
          n--;

          result += num + " ";
        }
        return result;
    }
}
